{
  "name": "dslink_dart_influxdb",
  "version": "1.0.0",
  "description": "Implementation of connection to the InfluxDB",
  "main": "bin/dslink.dart",
  "license": "Apache",
  "engines": {
    "dart": ">=3.1.3"
  },
  "getDependencies": [
    "pub get"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gencto/dslink-dart-influxdb"
  },
  "bugs": {
    "url": "https://github.com/gencto/dslink-dart-influxdb/issues"
  },
  "configs": {
    "broker": {
      "type": "url"
    },
    "name": {
      "type": "string",
      "default": "influxdb"
    },
    "token": {
      "type": "string"
    },
    "log": {
      "type": "enum",
      "default": "info"
    }
  }
}